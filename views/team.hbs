<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> 
<![endif]-->
<!--[if IE 7]> <html class="no-js lt-ie9 lt-ie8" lang="en"> 
<![endif]-->
<!--[if IE 8]> <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
    <head>
        <title>Dedicated Parents | Team</title>
        {{> header img=meta.img}}
    </head>
    <body>
        {{> headerNav active='staffs'}}
		<div class="container">
			<div class="page-header">
				<div class="row">
					<div class="col-md-6 col-sm-6">
						<h2 class="page-title">Out team</h2>
					</div> <!-- /.col-md-6 -->
					<div class="col-md-6 col-sm-6 hidden-xs back-home">
						<a href="/">&larr; Go back Home</a>
					</div> <!-- /.col-md-6 -->
				</div> <!-- /.row -->
			</div> <!-- /.page-header -->
			<div class="row">
                {{#each staffs}}
				<div class="col-md-4 col-sm-6">
					<div class="member-grid">
						<div class="member-thumb">
                            <img src="https://res.cloudinary.com/miscellaneous/image/upload/c_lpad,h_190,w_370/v1680079430/dedicatedparents/team-photos/{{this.image}}" alt="">
						</div> <!-- /.member-thumb -->
						<div class="member-content">
							<div class="pull-left">
                                <h3><a href="/team-member/{{this.slug}}">{{this.title}}</a></h3>
                                <span class="role">{{this.role}}</span>
							</div>
							<ul class="social pull-right">
                                {{#if this.twitter}}
                                    <li><a href="{{this.twitter}}" class="fa fa-twitter"></a></li>
                                {{/if}}
                                {{#if this.facebook}}
                                    <li><a href="{{this.facebook}}" class="fa fa-facebook"></a></li>
                                {{/if}}
                                {{#if this.googleplus}}
                                    <li><a href="{{this.googleplus}}" class="fa fa-google-plus"></a></li>
                                {{/if}}
                                {{#if this.linkedin}}
                                    <li><a href="{{this.linkedin}}" class="fa fa-linkedin"></a></li>
                                {{/if}}
							</ul>
							<div class="clearfix"></div>
                            <p>{{reduceStringLength this.detail '151'}}...</p>
                            <a href="/team-member/{{this.slug}}">Read More &rarr;</a>
						</div> <!-- /.member-content -->
					</div> <!-- /.member-grid -->
				</div> <!-- /.col-md-4 -->
                {{/each}}
			</div> <!-- /.row -->
		</div> <!-- /.container -->
        {{> footer}}
    </body>
</html>